{
  "name": "aws-cdk-ts-app-example",
  "version": "0.1.0",
  "license": "MIT",
  "bin": {
    "aws-cdk-app": "bin/app.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "synth:dev": "cdk synth --context environment=dev --no-version-reporting --output=./cdk.out",
    "synth:prod": "cdk synth --context environment=prod --no-version-reporting --output=./cdk.out",
    "deploy:dev": "cdk deploy --all --context environment=dev --no-version-reporting --require-approval never",
    "deploy:prod": "cdk deploy --all --context environment=prod --no-version-reporting --require-approval never",
    "destroy:dev": "cdk destroy --all --context environment=dev --force",
    "destroy:prod": "cdk destroy --all --context environment=prod --force"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "10.17.27",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.4",
    "aws-cdk": "^2.96.1",
    "ts-node": "^10.7.0",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "aws-cdk-lib": "2.96.1",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
}